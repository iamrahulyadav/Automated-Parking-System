<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.kushagr_jolly.potenza_pvt_ltd.Admin">




    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Masters"
        android:id="@+id/button_masters"
        android:layout_marginTop="48dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:onClick="masters"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Device Setting"
        android:id="@+id/button_device_setting"
        android:layout_above="@+id/button_vehicle_type"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="45dp"
        android:onClick="device_setting"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Reports"
        android:id="@+id/button_vehicle_type"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:onClick="reports"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Data Management"
        android:id="@+id/button_data_management"
        android:layout_marginBottom="51dp"
        android:layout_above="@+id/button_tariff"
        android:layout_centerHorizontal="true"
        android:onClick="data_management"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Transporter Accounts"
        android:id="@+id/button_tariff"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="40dp"
        android:onClick="transporter_accounts"/>

</RelativeLayout>
